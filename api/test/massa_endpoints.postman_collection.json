							"pm.test(\"Having two address\", function () {",
							"    var jsonData = (pm.response.json()).addressesAttributes;",
							"    pm.expect(Object.keys(jsonData).length).to.eql(2);",
							"});",
							"",
							"pm.test(\"Having final balances\", function () {",
							"    var jsonData = (pm.response.json()).addressesAttributes;",
