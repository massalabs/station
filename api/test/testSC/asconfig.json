{
    "targets": {
        "debug": {
            "outFile": "build/main.wasm",
            "sourceMap": true,
            "debug": true
        },
        "release": {
            "outFile": "build/main.wasm",
            "sourceMap": true,
            "optimizeLevel": 3,
            "shrinkLevel": 0,
            "converge": false,
            "noAssert": false
        }
    },
    "options": {
        "exportRuntime": true,
        "bindings": "esm",
        "transform": [
            "@massalabs/as-transformer"
        ]
    }
}